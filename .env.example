# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token_here
CLIENT_ID=your_discord_client_id_here

# Environment
NODE_ENV=development
LOG_LEVEL=info

GOOGLE_API_KEY=your_google_api_key_here

# Server Configuration
