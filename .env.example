# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token_here
CLIENT_ID=your_discord_client_id_here

# Environment
NODE_ENV=development
LOG_LEVEL=info

GOOGLE_API_KEY=your_google_api_key_here

# Server Configuration
PORT=3000
HEALTH_CHECK_PORT=3001

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100
COMMAND_COOLDOWN_SECONDS=30

# Bot Activities (comma-separated, optional)
# BOT_ACTIVITIES=Ready to help!,Serving servers,Type / to see commands

# Gemini Safety Settings (optional)
# Valid values: OFF, BLOCK_NONE, BLOCK_ONLY_HIGH, BLOCK_MEDIUM_AND_ABOVE, BLOCK_LOW_AND_ABOVE
# GEMINI_SAFETY_HARASSMENT=BLOCK_MEDIUM_AND_ABOVE
# GEMINI_SAFETY_HATE_SPEECH=BLOCK_MEDIUM_AND_ABOVE
# GEMINI_SAFETY_SEXUALLY_EXPLICIT=BLOCK_MEDIUM_AND_ABOVE
# GEMINI_SAFETY_DANGEROUS_CONTENT=BLOCK_MEDIUM_AND_ABOVE
# GEMINI_SAFETY_CIVIC_INTEGRITY=BLOCK_MEDIUM_AND_ABOVE